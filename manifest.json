{
  "name": "Close Other Tabs",
  "description": "Closes Other Tabs except active one. Press Alt+Shift+W to close other tabs with keyboard",
  "version": "0.9",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "images/closeOthers16.png",
      "32": "images/closeOthers32.png",
      "48": "images/closeOthers48.png",
      "128": "images/closeOthers128.png"
    },
    "default_title": "Close Other Tabs (Alt+Shift+W)"
  },
  "commands": {
    "close-other-tabs": {
      "suggested_key": {
        "default": "Alt+Shift+W"
      },
      "description": "Close Other Tabs"
    }
  },
  "icons": {
    "16": "images/closeOthers16.png",
    "32": "images/closeOthers32.png",
    "48": "images/closeOthers48.png",
    "128": "images/closeOthers128.png"
  }
}
