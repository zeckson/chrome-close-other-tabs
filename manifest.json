{
  "name": "Close Other Tabs",
  "description": "Closes Other Tabs except active one. Press Ctrl+Shift+W (Command+Shift+W on a Mac) to close other tabs with keyboard",
  "version": "0.5",
  "manifest_version": 2.1,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "images/closeOthers32.png",
    "default_title": "Press Ctrl(Win)/Command(Mac)+Shift+W to Close Other Tabs"
  },
  "commands": {
    "close-other-tabs": {
      "suggested_key": {
        "default": "Ctrl+Shift+W",
        "mac": "Command+Shift+W"
      },
      "description": "Close Other Tabs"
    }
  },
  "icons": {
    "16": "images/closeOthers16.png",
    "32": "images/closeOthers32.png",
    "48": "images/closeOthers48.png",
    "128": "images/closeOthers128.png"
  }
}
